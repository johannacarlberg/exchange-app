{
    "extends": [
        "airbnb"
    ],
  "rules": {
    "arrow-parens": "off",
    "no-underscore-dangle": "off",
    "react/destructuring-assignment": "off",
    "prefer-destructuring": "off"
  },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
      }
}
